<filter name="insert_term" module="contact" act="procContactAdminInsertTerm" confirm_msg_code="confirm_submit">
	<form>
	<node target="term"/>
	</form>
	<parameter>
		<param name="contact_name" target="mid" />
		<param name="module_srl" target="module_srl" />
		<param name="module_category_srl" target="module_category_srl" />
		<param name="layout_srl" target="layout_srl" />
		<param name="skin" target="skin" />
		<param name="browser_title" target="browser_title" />
		<param name="description" target="description" />
		<param name="term" target="term" />
		<param name="agree_text" target="agree_text" />
	</parameter>
	<response callback_func="completeTermInserted">
		<tag name="error" />
		<tag name="message" />
		<tag name="module" />
		<tag name="act" />
		<tag name="page" />
		<tag name="module_srl" />
	</response>
</filter>
